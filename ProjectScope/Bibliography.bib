	J1 = {Comput Phys Commun},
	Journal = {Computer physics communications},
	Keywords = {GPU, Automatic Differentiation, Math, Neural Network},
	Month = {03},
	Pages = {300--311},
	Title = {GPU-Accelerated Adjoint Algorithmic Differentiation},
	Ty = {JOUR},
